<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>JoinService</servlet-name>
		<display-name>JoinService</display-name>
		<description></description>
		<servlet-class>com.plandly.controller.JoinService</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>KakaoJoin</servlet-name>
		<display-name>KakaoJoin</display-name>
		<description></description>
		<servlet-class>com.plandly.controller.KakaoJoin</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>MemberJoin</servlet-name>
		<display-name>MemberJoin</display-name>
		<description></description>
		<servlet-class>com.plandly.controller.MemberJoin</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Plandly_MemberVO</servlet-name>
		<display-name>Plandly_MemberVO</display-name>
		<description></description>
		<servlet-class>com.plandly.model.Plandly_MemberVO</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Plandly_MemberVO</servlet-name>
		<url-pattern>/Plandly_MemberVO</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>KakaoJoin</servlet-name>
		<url-pattern>/JoinServlet</url-pattern> <!-- KakaoLogin servlet annotation과 동일하게 -->
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>MemberJoin</servlet-name>
		<url-pattern>/MemberJoin</url-pattern>
	</servlet-mapping>


</web-app>
